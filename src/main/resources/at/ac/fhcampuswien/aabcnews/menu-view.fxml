<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" prefHeight="277.0" prefWidth="425.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.ac.fhcampuswien.aabcnews.AppController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <ListView />
        <VBox prefHeight="238.0" prefWidth="99.0" spacing="10.0">
            <children>
                <Label fx:id="welcomeText" text="AABCNews" />
                <ChoiceBox prefWidth="150.0" />
                <Button onAction="#onHelloButtonClick" prefHeight="30.0" prefWidth="100.0" text="Get News" />
            <Button mnemonicParsing="false" text="Quit" />
            </children>
        </VBox>
    </children>
</HBox>
